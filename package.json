    "ava": "^3.8.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.0.0",
    "nyc": "^15.0.1",
    "ts-loader": "^7.0.3",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
